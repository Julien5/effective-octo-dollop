ifeq ($(firstword $(MAKEFILE_LIST)),$(XNAME))
	BUILDME=1
endif

ifndef MAKEFILESDIR
include ../settings
endif
$(info C $(MAKEFILE_LIST))

include $(MAKEFILESDIR)/arduino/LowPower

XNAME:=common
XINCLUDE += -I$(DIREMBEDDED)/$(XNAME)
XLIBS += -L/tmp/build/arduino/$(XNAME)/ -l$(XNAME)

$(info C $(lastword $(MAKEFILE_LIST)))

ifndef NAME
include settings
NAME:=$(XNAME)
# build deps and me.
all: deps lib

deps:
	make -C $(MAKEFILESDIR)/arduino -f LowPower lib

INCLUDE += -I/usr/share/arduino/libraries/EEPROM/
SRCSDIR := $(DIREMBEDDED)/$(NAME)
include build.mk

endif

$(info C $(lastword $(MAKEFILE_LIST)))
$(info C $(words $(MAKEFILE_LIST)))
